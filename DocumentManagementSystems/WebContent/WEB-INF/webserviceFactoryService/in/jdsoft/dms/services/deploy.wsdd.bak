<!--      from the same directory that the Axis engine runs         -->

<deployment
    xmlns="http://xml.apache.org/axis/wsdd/"
    xmlns:java="http://xml.apache.org/axis/wsdd/providers/java">

  <!-- Services from webserviceFactoryService WSDL service -->

  <service name="webserviceFactory" provider="java:RPC" style="wrapped" use="literal">
      <parameter name="wsdlTargetNamespace" value="http://services.dms.jdsoft.in"/>
      <parameter name="wsdlServiceElement" value="webserviceFactoryService"/>
      <parameter name="schemaQualified" value="http://services.dms.jdsoft.in"/>
      <parameter name="wsdlServicePort" value="webserviceFactory"/>
      <parameter name="className" value="in.jdsoft.dms.services.WebserviceFactorySoapBindingImpl"/>
      <parameter name="wsdlPortType" value="webserviceFactory"/>
      <parameter name="typeMappingVersion" value="1.2"/>
      <operation name="getInstance" qname="operNS:getInstance" xmlns:operNS="http://services.dms.jdsoft.in" returnQName="retNS:getInstanceReturn" xmlns:retNS="http://services.dms.jdsoft.in" returnType="rtns:anyType" xmlns:rtns="http://www.w3.org/2001/XMLSchema" soapAction="" >
        <parameter qname="pns:UserName" xmlns:pns="http://services.dms.jdsoft.in" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
        <parameter qname="pns:Password" xmlns:pns="http://services.dms.jdsoft.in" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <parameter name="allowedMethods" value="getInstance"/>

  </service>
</deployment>
